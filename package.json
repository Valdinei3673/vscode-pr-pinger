{
  "name": "vscode-pr-pinger",
  "displayName": "PR Pinger",
  "publisher": "jrieken",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.67.0"
  },
  "description": "",
  "repository": {
    "url": "https://github.com/jrieken/vscode-pr-pinger"
  },
  "browser": "dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "vscode:prepublish": "npm run typecheck && npm run transpile",
    "transpile": "npx esbuild --bundle src/extension.ts --outfile=dist/extension.js --external:vscode --target=es2021 --format=cjs  --sourcemap=external",
    "typecheck": "tsc -p ./src/ --noEmit"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.15.10",
    "typescript": "^4.4.3",
    "@octokit/graphql": "^5.0.1"
  }
}
